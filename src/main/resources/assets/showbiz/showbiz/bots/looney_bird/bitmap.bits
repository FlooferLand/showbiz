set rae looney_bird

rae head_right {
    flow 0.25
    rotate "head" y:-78
    wigglemul 2.0
}

rae jaw {
    flow 1.5
    rotate "mouth" x:-25
}

rae raise {
    flow linear 0.25
    move "looney" y:11
    wigglemul 2.0
}

rae left_eyelid {
    flow 0.8
    move "eyelid_l" y:-2
}

rae right_eyelid {
    flow 0.8
    move "eyelid_r" y:-2
}

rae eye_cross {
    flow 1.0
    anim "eyes_cross"
}
